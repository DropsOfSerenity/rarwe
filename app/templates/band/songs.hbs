<div class="row">
  <div class="list-group songs">
    <li class="list-group-item">
      {{input type="text" class="new-band" placeholder="New Song" value=title}}
      <button class="btn btn-primary btn-sm new-song-button" {{action "createSong"}}>Add</button>
    </li>
    {{#each model as |song|}}
      <li class="list-group-item song">
        {{song.title}}
        {{star-rating item=song rating=song.rating setAction="updateRating"}}
      </li>
    {{/each}}
  </div>
</div>
